{
  "manifest_version": 3,
  "name": "Instagram Unfollower",
  "version": "1.0",
  "description": "Unfollows users on Instagram who don't follow you back.",
  "permissions": ["activeTab", "scripting","storage"],
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["content.js"]
    }
  ],
        "icons": {
          "16": "icons/icon16.png",
          "32": "icons/icon32.png",
          "48": "icons/icon48.png",
          "128": "icons/icon128.png"
        }
}
